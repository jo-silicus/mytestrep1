<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
    
<hibernate-mapping>
<!-- 
    Created by the Middlegen Hibernate plugin 2.2

    http://boss.bekk.no/boss/middlegen/
    http://www.hibernate.org/
-->

<class 
    name="com.mgmtassessment.execmap.data.dao.types.ReportMaster" 
    table="rpt_mas"
    lazy="false"
>

    <id
        name="rptId"
        type="java.lang.Short"
        column="rpt_id"
    >
    
        <generator class="assigned" />
    </id>

    <property
        name="rptDesc"
        type="java.lang.String"
        column="rpt_desc"
        not-null="true"
        length="40"
    />
    <property
        name="version"
        type="java.lang.Integer"
        column="version"
        length="10"
    />

    <!-- Associations -->
  
    <!-- bi-directional one-to-many association to CoAcctMa -->

    <set
        name="coAcctMas"
        lazy="true"
        inverse="true"
		cascade="all"

    >
        <key>
            <column name="rpt_id" />
        </key>
        
        <one-to-many 
            class="com.mgmtassessment.execmap.data.dao.types.CompanyAcctMaster"
        />
    </set>
    <!-- bi-directional one-to-many association to RptParaDet -->

    <set
        name="rptParaDets"
        lazy="true"
        inverse="true"
		cascade="all"

    >
        <key>
            <column name="rpt_id" />
        </key>
        
        <one-to-many 
            class="com.mgmtassessment.execmap.data.dao.types.RptParaDet"
        />
    </set>

</class>
</hibernate-mapping>
